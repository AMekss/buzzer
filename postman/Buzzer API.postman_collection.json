        "body": {
          "mode": "graphql",
          "graphql": {
            "query": "query conversation($title: String!) {\n    conversation(title: $title) {\n        title\n        messagesConnection(first: 10) {\n            nodes {\n                from {\n                    fullName\n                }\n                text\n            }\n            pageInfo {\n                startCursor\n                endCursor\n            }\n        }\n    }\n}",
            "variables": "{\n    \"title\": \"Ruby community chat\"\n    \n}"
          }
        },
